    <artifactId>interface-parent</artifactId>
    <version>0.1.0-SNAPSHOT</version>
  </parent>

  <artifactId>interface-test</artifactId>
  <name>MaUI Test Guide :: UT Example - Test</name>

  <!--  START SNIPPET: start -->
  <build>
